extends layout

block content
    .upload-page
        h1.page-header
            上传契约    
        - if(!result)
            form(action="upload", method="post", enctype = "multipart/form-data")
              input(type="file", name="contractfile")
              input.btn.btn-sm.btn-success(type="submit", value="上传")
        - else
            p= result
